@section Search {	
	@Html.Partial("_ApiSearch")
}

@{
	Layout = "/_Layout.cshtml";
	@Html.Partial("Section\\_DocsTable", new DocsTable
	{
		Docs = Documents.FromPipeline(Docs.Api)
			.Where(x => x.String("Kind") == "Namespace")
			.OrderBy(x => x.String("DisplayName"))
			.ToList(),
		Title = "Namespaces",
		Header = "Namespace",
		HasSummary = true,
		LinkTypeArguments = false	
	});
}  