Title: Reference
NoSidebar: true
---

<section class="content">
	<div class="box">
		<div class="box-body no-padding">
			<table class="table table-striped table-hover table-responsive">
				<tbody>
					<tr>
						<td>
							<strong><a href="/docs/overview">Introduction</a></strong>
							<p>Requirements, installation, quick start, history of NAnt and other miscellaneous topics.</p>
						</td>
					</tr>
					<tr>
						<td>
							<strong><a href="/docs/fundamentals">Fundamentals</a></strong>
							<p>The basic concepts needed to understand and use NAnt.</p>
						</td>
					</tr>
					@{
						IEnumerable<IDocument> refs = Documents.FromPipeline("ReferenceTypes").OrderBy(x => x.String(DocsKeys.Order));
						foreach(IDocument typeDoc in refs)
						{
							<tr>
								<td>
									<strong><a href="@Context.GetLink(typeDoc)">@(typeDoc.String(Keys.Title))</a></strong>
									<p>@Html.Raw(typeDoc.String(DocsKeys.Description))</p>
								</td>
							</tr>
						}
					}
				</tbody>
			</table>
		</div>
	</div>
</section>